{
  "name": "patronum",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test pnpm test:code && pnpm test:types",
    "test:code": "jest",
    "test:types": "tsc -p ./test-typings",
    "format": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "lint": "eslint ./",
    "cleanup": "rm -rf dist && mkdir dist",
    "build:indexes": "node scripts/build.js",
    "build:types": "tsc --project ./tsconfig.build.json",
    "build:commonjs": "pnpm babel --verbose --out-dir dist src -x .ts --ignore '**/*.test.ts','**/*.d.ts' --out-file-extension .cjs",
    "build:mjs": "ESMODULES=true pnpm build:commonjs --out-file-extension .js",
    "build:bundles": "rollup -c",
    "build": "pnpm cleanup && pnpm build:indexes && pnpm build:types && pnpm build:commonjs && pnpm build:mjs && pnpm build:bundles",
    "prepublishOnly": "pnpm build:methods && yarn build:cjs"
  },
  "workspaces": [
    "website"
  ],
  "author": "Sergey Sova <mail@sergeysova.com> (https://sergeysova.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-terser": "^0.2.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^18.11.11",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-tester": "^10.0.0",
    "camel-case": "^4.1.1",
    "effector": "^22.5.0",
    "globby": "^11.0.0",
    "husky": "8.0.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "regenerator-runtime": "^0.13.11",
    "rollup": "^2.79.1",
    "tsd": "^0.13.1",
    "typescript": "4.7.4",
    "zx": "^7.1.1"
  },
  "packageManager": "pnpm@7.9.5",
  "optionalDependencies": {
    "svelte": "^3.54.0"
  }
}
